<h1>New Photo</h1>


<%= form_for @photo, :html => {:multipart => true} do |f| %>

  <div>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </div>

    <div>
    <%= f.label :approved %><br />
    <%= f.check_box :approved? %>
  </div>

  <div>
    <%= f.label :user_id %><br />
    <%= f.number_field :user_id %>
   </div>

  <div>
    <%= f.file_field :image %>
  </div>

    <%= f.submit "Create Photo" %>

<% end %>
