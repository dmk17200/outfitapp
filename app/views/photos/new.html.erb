<h1>New Photo</h1>


<%= form_tag(photos_url, method: 'post') do %>


  <div>
    <%= label_tag :description %><br />
    <%= text_field_tag :description %>
  </div>

    <div>
    <%= label_tag :approved %><br />
    <%= check_box_tag :approved? %>
  </div>

    <div>
    <%= label_tag :user_id %><br />
    <%= number_field_tag :user_id %>
  </div>

  
  <div>
    <%= submit_tag "Create Photo" %>
  </div>
<% end %>
